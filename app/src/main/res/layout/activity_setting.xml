<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    android:background="#222425"
    tools:context=".settingActivity">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <LinearLayout
            android:layout_width="@dimen/_280sdp"
            android:layout_height="@dimen/_40sdp"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/_10sdp"
            android:orientation="horizontal">
            <ImageView
                android:id="@+id/cancel"
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_36ssp"
                android:src="@drawable/cancel"
                android:layout_marginTop="@dimen/_7sdp"
                android:layout_marginStart="@dimen/_10sdp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Settings"
                android:textSize="@dimen/_20ssp"
                android:textStyle="normal"
                android:textColor="#ffffff"
                android:layout_marginStart="@dimen/_20sdp"
                android:layout_marginTop="@dimen/_7sdp"/>

        </LinearLayout>

       <LinearLayout
           android:id="@+id/settin"
           android:layout_width="@dimen/_280sdp"
           android:layout_height="@dimen/_40sdp"
           android:background="@drawable/backkkk"
           android:layout_gravity="center"
           android:layout_marginTop="@dimen/_50sdp"
           android:orientation="horizontal">
           <ImageView
               android:layout_width="@dimen/_30sdp"
               android:layout_height="@dimen/_20ssp"
               android:src="@drawable/settings"
               android:layout_marginTop="@dimen/_7sdp"
               android:layout_marginStart="@dimen/_10sdp"/>
           <TextView
               android:layout_width="wrap_content"
               android:layout_height="wrap_content"
               android:text="Setting"
               android:textSize="@dimen/_14ssp"
               android:textStyle="normal"
               android:textColor="#ffffff"
               android:layout_marginStart="@dimen/_3sdp"
               android:layout_marginTop="@dimen/_7sdp"/>

       </LinearLayout>
        <LinearLayout
            android:id="@+id/getschedule"
            android:layout_width="@dimen/_280sdp"
            android:layout_height="@dimen/_40sdp"
            android:background="@drawable/backkkk"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/_10sdp"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_20ssp"
                android:src="@drawable/get"
                android:layout_marginTop="@dimen/_7sdp"
                android:layout_marginStart="@dimen/_10sdp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Get Scedule"
                android:textSize="@dimen/_14ssp"
                android:textStyle="normal"
                android:textColor="#ffffff"
                android:layout_marginStart="@dimen/_3sdp"
                android:layout_marginTop="@dimen/_7sdp"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/sendschedule"
            android:layout_width="@dimen/_280sdp"
            android:layout_height="@dimen/_40sdp"
            android:background="@drawable/backkkk"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/_10sdp"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_20ssp"
                android:src="@drawable/send"
                android:layout_marginTop="@dimen/_7sdp"
                android:layout_marginStart="@dimen/_10sdp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Send Schedule"
                android:textSize="@dimen/_14ssp"
                android:textStyle="normal"
                android:textColor="#ffffff"
                android:layout_marginStart="@dimen/_3sdp"
                android:layout_marginTop="@dimen/_7sdp"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/support"
            android:layout_width="@dimen/_280sdp"
            android:layout_height="@dimen/_40sdp"
            android:background="@drawable/backkkk"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/_10sdp"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_20ssp"
                android:src="@drawable/support"
                android:layout_marginTop="@dimen/_7sdp"
                android:layout_marginStart="@dimen/_10sdp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Support"
                android:textSize="@dimen/_14ssp"
                android:textStyle="normal"
                android:textColor="#ffffff"
                android:layout_marginStart="@dimen/_3sdp"
                android:layout_marginTop="@dimen/_7sdp"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/appearance"
            android:layout_width="@dimen/_280sdp"
            android:layout_height="@dimen/_40sdp"
            android:background="@drawable/backkkk"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/_10sdp"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_20ssp"
                android:src="@drawable/appearnce"
                android:layout_marginTop="@dimen/_7sdp"
                android:layout_marginStart="@dimen/_10sdp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Appearancd"
                android:textSize="@dimen/_14ssp"
                android:textStyle="normal"
                android:textColor="#ffffff"
                android:layout_marginStart="@dimen/_3sdp"
                android:layout_marginTop="@dimen/_7sdp"/>

        </LinearLayout>
        <LinearLayout
            android:id="@+id/pro"
            android:layout_width="@dimen/_280sdp"
            android:layout_height="@dimen/_40sdp"
            android:background="@drawable/backkkk"
            android:layout_gravity="center"
            android:layout_marginTop="@dimen/_10sdp"
            android:orientation="horizontal">
            <ImageView
                android:layout_width="@dimen/_30sdp"
                android:layout_height="@dimen/_20ssp"
                android:src="@drawable/pro"
                android:layout_marginTop="@dimen/_7sdp"
                android:layout_marginStart="@dimen/_10sdp"/>
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="pro"
                android:textSize="@dimen/_14ssp"
                android:textStyle="normal"
                android:textColor="#ffffff"
                android:layout_marginStart="@dimen/_3sdp"
                android:layout_marginTop="@dimen/_7sdp"/>

        </LinearLayout>

    </LinearLayout>



</androidx.constraintlayout.widget.ConstraintLayout>